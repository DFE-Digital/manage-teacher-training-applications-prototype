    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "inquirer": "^8.2.6",
    "keypather": "^3.1.0",
    "luxon": "^3.5.0",
    "marked": "^14.1.3",
    "numeral": "^2.0.6",
    "nunjucks": "^3.2.4",
    "pluralize": "^8.0.0",
    "portscanner": "^2.1.1",
    "require-dir": "^1.0.0",
    "sass": "^1.80.4",
    "sync-request": "^6.1.0",
    "universal-analytics": "^0.5.1",
    "uuid": "^8.3.2",
    "weighted": "^1.0.0"
  },
  "greenkeeper": {
    "ignore": [
      "nunjucks"
    ]
  },
  "devDependencies": {
    "glob": "^7.1.4",
