    "ansi-colors": "^4.1.1",
    "archiver-promise": "^1.0.0",
    "basic-auth": "^2.0.0",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.19.0",
    "browser-sync": "^2.26.13",
    "client-sessions": "^0.8.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.7",
    "cross-spawn": "^7.0.3",
    "csv-writer": "^1.6.0",
    "del": "^6.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-writer": "^0.0.4",
    "fancy-log": "^2.0.0",
